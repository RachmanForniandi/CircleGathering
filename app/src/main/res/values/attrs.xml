<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TextViewUsage">
        <attr name="custom_type" format="enum">
            <enum name="email" value="0" />
            <enum name="password" value="1" />
            <enum name="name" value="2" />
        </attr>
        <attr name="min_password_length" format="integer" />
    </declare-styleable>


    <declare-styleable name="AppWidgetAttrs">
        <attr name="appWidgetPadding" format="dimension" />
        <attr name="appWidgetInnerRadius" format="dimension" />
        <attr name="appWidgetRadius" format="dimension" />
        <attr name="appWidgetBackgroundColor" format="color" />
        <attr name="appWidgetTextColor" format="color" />
    </declare-styleable>


</resources>